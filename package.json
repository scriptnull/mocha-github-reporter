{
  "name": "mocha-github-reporter",
  "version": "0.0.0",
  "description": "Reports Mocha test results as github issue.",
  "main": "index.js",
  "scripts": {
    "test": "source dev.env && mocha --reporter . tests/dummy.js ",
    "test-all-pass": "source dev.env && mocha --reporter . tests/dummy-all-pass.js",
    "test-no-report": "source dev.env && mocha tests/dummy.js",
    "codestyle-check": "standard",
    "codestyle-fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scriptnull/mocha-github-reporter.git"
  },
  "keywords": [
    "mocha",
    "reporter",
    "github",
    "issues"
  ],
  "author": "Vishnu Bharathi <vishnubharathi04@gmail.com> (https://twitter.com/scriptnull)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scriptnull/mocha-github-reporter/issues"
  },
  "homepage": "https://github.com/scriptnull/mocha-github-reporter#readme",
  "dependencies": {
    "async": "^2.1.5",
    "bluebird": "^3.4.7",
    "left-pad": "^1.1.3",
    "mocha": "^3.2.0",
    "superagent": "^3.5.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "github-create-issue": "^2.0.0",
    "mocha": "^3.2.0",
    "standard": "^8.6.0"
  }
}
